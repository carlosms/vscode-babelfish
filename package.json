{
  "name": "vscode-uast-viewer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "carlosms",
  "activationEvents": [
    "onCommand:uast-viewer.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "uast-viewer.start",
        "title": "Parse UAST",
        "category": "Bblfsh"
      }
    ]
  },
  "dependencies": {
    "@types/google-protobuf": "^3.2.7",
    "codemirror": "^5.39.2",
    "google-protobuf": "^3.6.1",
    "grpc-web-client": "^0.6.2",
    "react": "^16.3.2",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.3.2",
    "react-json-view": "^1.19.1",
    "react-scripts-ts": "2.16.0",
    "uast-viewer": "^0.0.10",
    "vscode": "^1.1.17"
  },
  "scripts": {
    "vscode:prepublish": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3"
  }
}
