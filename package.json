{
  "name": "vscode-webview-react",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "rebornix",
  "activationEvents": [
    "onCommand:react-webview.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-webview.start",
        "title": "Start React Webview",
        "category": "React"
      }
    ]
  },
  "dependencies": {
    "codemirror": "^5.39.2",
    "react": "^16.3.2",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.16.0",
    "uast-viewer": "^0.0.10",
    "vscode": "^1.1.17"
  },
  "scripts": {
    "vscode:prepublish": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3"
  }
}
