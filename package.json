{
  "name": "vscode-uast-viewer",
  "homepage": "https://github.com/carlosms/vscode-uast-viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlosms/vscode-uast-viewer.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "carlosms",
  "activationEvents": [
    "onCommand:uast-viewer.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "uast-viewer.start",
        "title": "Parse UAST",
        "category": "Bblfsh"
      }
    ]
  },
  "dependencies": {
    "codemirror": "^5.41.0",
    "react": "^16.6.1",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.6.1",
    "react-scripts-ts": "2.16.0",
    "request": "^2.88.0",
    "uast-viewer": "^0.2.0",
    "vscode": "^1.1.21"
  },
  "scripts": {
    "vscode:prepublish": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "@types/request": "^2.48.1",
    "typescript": "^2.8.3"
  }
}
